						<prop oor:name="URL" oor:type="xs:string">
							<value>vnd.sun.star.script:oootranslit.fncTranslit.trSLC?language=Basic&amp;location=application</value>
						</prop>
						<prop oor:name="ImageIdentifier" oor:type="xs:string">
							<value>%origin%/images/slc</value>
						</prop>
						<prop oor:name="ImageBigURL" oor:type="xs:string">
							<value>%origin%/images/slc.png</value>
						</prop>
						<prop oor:name="ImageSmallURL" oor:type="xs:string">
							<value>%origin%/images/slc.png</value>
						</prop>
						<prop oor:name="Title" oor:type="xs:string">
							<value cml:lang="en">Convert selection to Cyrillic script</value>
							<value cml:lang="sr">Преслови само изабрано у ћирилицу</value>
							<value cml:lang="sr_RS">Preslovi samo izabrano u ćirilicu</value>
						</prop>
						<prop oor:name="Target" oor:type="xs:string">
							<value>_self</value>
						</prop>
					</node>
					<node oor:name="m6" oor:op="replace">
						<prop oor:name="Context" oor:type="xs:string">
							<value>com.sun.star.text.TextDocument,com.sun.star.sheet.SpreadsheetDocument</value>
						</prop>
						<prop oor:name="URL" oor:type="xs:string">
							<value>vnd.sun.star.script:oootranslit.fncTranslit.trSCL?language=Basic&amp;location=application</value>
						</prop>
						<prop oor:name="ImageIdentifier" oor:type="xs:string">
							<value>%origin%/images/scl</value>
						</prop>
						<prop oor:name="ImageBigURL" oor:type="xs:string">
							<value>%origin%/images/scl.png</value>
						</prop>
						<prop oor:name="ImageSmallURL" oor:type="xs:string">
							<value>%origin%/images/scl.png</value>
						</prop>
						<prop oor:name="Title" oor:type="xs:string">
							<value cml:lang="en">Convert selection to Cyrillic script</value>
							<value cml:lang="sr">Преслови само изабрано у ћирилицу</value>
							<value cml:lang="sr_RS">Preslovi samo izabrano u ćirilicu</value>
						</prop>
						<prop oor:name="Target" oor:type="xs:string">
							<value>_self</value>
						</prop>
					</node>
				</node>
			</node>
		</node>
	</node>
</oor:component-data>
